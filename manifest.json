{
	"manifest_version": 3,
	"name": "Download EPITA Tree",
	"version": "1.0",
	"description": "Allows to download an EPITA tree as a zip archive from the intranet",
	"icons": {
		"16": "icons/16.png",
		"32": "icons/32.png",
		"48": "icons/48.png",
		"128": "icons/128.png"
	},
	"background": {
		"service_worker": "background.js"
	},
	"permissions": [
		"tabs",
		"contextMenus",
		"downloads",
		"downloads.shelf",
		"downloads.open",
		"storage",
		"webRequest",
		"declarativeNetRequest",
		"declarativeNetRequestWithHostAccess",
		"activeTab",
		"scripting",
		"webNavigation"
	],
	"host_permissions": [
		"*://intra.forge.epita.fr/*"
	],
	"content_scripts": []
}
